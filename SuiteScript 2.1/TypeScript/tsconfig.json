{
  "compilerOptions": {
    "module": "AMD",
    "target": "ES2023",
    "moduleResolution": "node",
    "sourceMap": false,
    "skipLibCheck": true,
    "newLine": "LF",
    "experimentalDecorators": true,
    "noUncheckedIndexedAccess": true,
    "noFallthroughCasesInSwitch": true,
    "baseUrl": ".",
    "paths": {
      "N": [
        "node_modules/@hitc/netsuite-types/N"
      ],
      "N/*": [
        "node_modules/@hitc/netsuite-types/N/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}